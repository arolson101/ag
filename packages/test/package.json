{
  "name": "electron-webpack-quick-start",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "electron-builder": "electron-builder install-app-deps"
  },
  "electronWebpack": {
    "title": true,
    "whiteListedModules": ["@ag/app"],
    "main": {
      "sourceDirectory": "src/main",
      "webpackConfig": "webpack.additions.js"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "webpackConfig": "webpack.additions.js"
    }
  },
  "dependencies": {
    "@ag/app": "^0.0.0",
    "@blueprintjs/core": "^3.11.0",
    "@blueprintjs/icons": "^3.4.0",
    "@blueprintjs/select": "^3.4.0",
    "classnames": "^2.2.6",
    "commander": "^2.19.0",
    "debug": "^4.1.1",
    "electron": "^4.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.0.1",
    "electron-window-state": "^5.0.3",
    "formik": "^1.4.2",
    "history": "^4.7.2",
    "normalize.css": "^8.0.1",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-observable": "^1.0.0",
    "source-map-support": "^0.5.10",
    "sqlite3": "4.0.0",
    "typeorm": "^0.2.11",
    "typesafe-actions": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/classnames": "^2.2.7",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-window-state": "^2.0.33",
    "@types/history": "^4.7.2",
    "@types/query-string": "^6.2.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/sqlite3": "^3.1.3",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-links": "^3.3.3",
    "@types/storybook__react": "^4.0.0",
    "electron-builder": "20.38.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-react-devtools": "^0.5.3",
    "electron-webpack": "^2.6.2",
    "electron-webpack-ts": "^3.1.1",
    "html-loader": "^0.5.5",
    "typescript": "^3.2.4",
    "webpack": "4.28.4",
    "webpack-build-notifier": "^0.1.30"
  }
}
