{
  "name": "storybook",
  "private": true,
  "version": "0.0.0",
  "main": "electron.js",
  "build": {
    "appId": "com.example.ag"
  },
  "scripts": {
    "android": "cross-env-shell react-native run-android --no-packager --config $INIT_CWD/rn-cli.config.js",
    "bundle-android": "cross-env-shell react-native bundle --config $INIT_CWD/rn-cli.config.js --platform android --dev true --entry-file $INIT_CWD/index.js --bundle-output $INIT_CWD/dist/bundle.android.dev.js",
    "bundle-ios": "cross-env-shell react-native bundle --config $INIT_CWD/rn-cli.config.js --platform ios --dev true --entry-file $INIT_CWD/index.js --bundle-output $INIT_CWD/dist/bundle.ios.dev.js",
    "electron": "concurrently --raw --kill-others \"yarn:electron:server\" \"yarn:electron:main\"",
    "electron:main": "wait-on http://localhost:6006/ && cross-env DEBUG=* electron . --url http://localhost:6006/",
    "electron:server": "start-storybook --ci --quiet -p 6006",
    "preios": "cd ./ios && pod install",
    "ios": "cross-env-shell react-native run-ios --no-packager --config $INIT_CWD/rn-cli.config.js",
    "start": "cross-env-shell react-native start --config $INIT_CWD/rn-cli.config.js",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "@ag/core": "^0.0.0",
    "@ag/online": "^0.0.0",
    "@ag/ui-antd": "^0.0.0",
    "@ag/ui-nativebase": "^0.0.0",
    "@ag/util": "^0.0.0",
    "@babel/runtime": "^7.4.4",
    "@emotion/core": "^10.0.10",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-ondevice-knobs": "^5.0.0-beta.4",
    "@storybook/react": "^5.0.11",
    "@storybook/react-native": "^5.0.0-beta.4",
    "@storybook/theming": "^5.0.11",
    "apollo-link-mock": "^1.0.1",
    "babel-runtime": "^7.0.0-beta.3",
    "commander": "^2.20.0",
    "debug": "^4.1.1",
    "electron": "^5.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-window-state": "^5.0.3",
    "emotion-theming": "^10.0.10",
    "native-base": "^2.12.1",
    "node-libs-react-native": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-native": "^0.59.5",
    "react-native-fs": "^2.13.3",
    "react-native-sqlite-storage": "^3.3.10",
    "react-native-stream": "^0.1.9",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-window-state": "^2.0.33",
    "@types/react": "^16.8.16",
    "@types/react-intl": "^2.3.17",
    "@types/react-native": "^0.57.51",
    "@types/react-native-fs": "^2.8.3",
    "@types/react-native-sqlite-storage": "^3.3.1",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__react": "^4.0.1",
    "@types/storybook__react-native": "^3.0.4",
    "@types/webpack": "^4.4.31",
    "css-loader": "^2.1.1",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.0",
    "react-native-typescript-transformer": "^1.2.12",
    "style-loader": "^0.23.1",
    "ts-transform-graphql-tag": "^0.2.1",
    "wait-on": "^3.2.0",
    "webpack": "^4.30.0"
  }
}
