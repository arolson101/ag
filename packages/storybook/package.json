{
  "name": "storybook",
  "private": true,
  "version": "0.0.0",
  "main": "electron.js",
  "build": {
    "appId": "com.example.ag"
  },
  "scripts": {
    "android": "react-native run-android",
    "bundle-ios": "yarn run react-native bundle --platform ios --dev true --entry-file index.js --bundle-output dist/bundle.ios.dev.js",
    "electron": "concurrently --raw --kill-others \"yarn:electron:server\" \"yarn:electron:main\"",
    "electron:main": "wait-on http://localhost:6006/ && electron . --url http://localhost:6006/",
    "electron:server": "start-storybook --ci --quiet -p 6006",
    "preios": "cd ./ios && pod install",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "@ag/core": "^0.0.0",
    "@ag/react-native-fetch": "^0.0.0",
    "@ag/ui-blueprint": "^0.0.0",
    "@ag/ui-nativebase": "^0.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addon-ondevice-knobs": "^4.1.13",
    "@storybook/react": "^4.1.4",
    "@storybook/react-native": "^4.1.4",
    "commander": "^2.19.0",
    "debug": "^4.1.1",
    "electron": "^4.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-window-state": "^5.0.3",
    "native-base": "^2.10.0",
    "node-libs-react-native": "^1.0.3",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-intl": "^2.7.2",
    "react-native": "^0.57.8",
    "react-native-fs": "^2.13.3",
    "react-native-sqlite-storage": "^3.3.9",
    "react-native-stream": "^0.1.9",
    "typeorm": "0.2.11"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-window-state": "^2.0.33",
    "@types/react": "^16.7.18",
    "@types/react-intl": "^2.3.14",
    "@types/react-native": "^0.57.32",
    "@types/react-native-fs": "^2.8.2",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-links": "^3.3.3",
    "@types/storybook__react": "^4.0.0",
    "@types/storybook__react-native": "^3.0.4",
    "babel-core": "^7.0.0-0",
    "jest": "^23.6.0",
    "metro-react-native-babel-preset": "^0.45.0",
    "react-native-typescript-transformer": "^1.2.11"
  }
}
