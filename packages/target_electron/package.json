{
  "name": "app_electron",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "start": "nodemon -q --watch ./webpack.config.ts --exec \"webpack-dev-server --port 3030 --hot --mode=development\"",
    "electron": "wait-on ./dist/main.js && nodemon -q --watch ./dist/main.js --exec \"electron dist --url http://localhost:3030\"",
    "start:dev": "rimraf dist && concurrently --kill-others \"npm:start\" \"npm:electron\"",
    "storybook": "concurrently --raw --kill-others \"npm:storybook:server\" \"npm:storybook:electron\"",
    "storybook:server": "start-storybook --ci --quiet -p 6006",
    "storybook:electron": "wait-on http://localhost:6006 && electron dist --url http://localhost:6006/"
  },
  "dependencies": {
    "@ag/app": "^0.0.0",
    "@ag/testlib": "^0.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-window-state": "^2.0.33",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "commander": "^2.19.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.0",
    "electron-is-dev": "^1.0.1",
    "electron-react-devtools": "^0.5.3",
    "electron-window-state": "^5.0.3"
  }
}
