{
  "name": "target_electron",
  "version": "0.0.0",
  "description": "description",
  "license": "MIT",
  "author": "Andrew Olson <andrew@olsononline.org>",
  "build": {
    "appId": "com.example.ag"
  },
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "preelectron": "electron-builder install-app-deps",
    "electron": "wait-on ./dist/main.js && nodemon -q --watch ./dist/main.js --exec \"electron dist --url http://localhost:3030/\"",
    "dev-server": "nodemon -q --watch ./webpack.config.ts --exec \"webpack-dev-server --port 3030 --hot --mode=development\"",
    "start:dev": "rimraf dist && concurrently --kill-others \"yarn:dev-server\" \"yarn:electron\"",
    "storybook": "concurrently --raw --kill-others \"yarn:storybook:server\" \"yarn:storybook:electron\"",
    "storybook:electron": "wait-on http://localhost:6006/webpack-dev-server/ && electron dist --url http://localhost:6006/webpack-dev-server/",
    "storybook:server": "start-storybook --ci --quiet -p 6006"
  },
  "dependencies": {
    "@ag/app": "^0.0.0",
    "@blueprintjs/core": "^3.11.0",
    "@blueprintjs/icons": "^3.4.0",
    "@blueprintjs/select": "^3.4.0",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link-http": "^1.5.9",
    "commander": "^2.19.0",
    "debug": "^4.1.1",
    "electron": "^4.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.0.1",
    "electron-window-state": "^5.0.3",
    "formik": "^1.4.2",
    "history": "^4.7.2",
    "normalize.css": "^8.0.1",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.3.3",
    "sqlite3": "4.0.0",
    "typeorm": "^0.2.11",
    "typesafe-actions": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-window-state": "^2.0.33",
    "@types/history": "^4.7.2",
    "@types/query-string": "^6.2.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/sqlite3": "^3.1.3",
    "devtron": "^1.4.0",
    "electron-builder": "^20.38.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-react-devtools": "^0.5.3"
  }
}
