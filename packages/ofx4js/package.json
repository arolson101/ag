{
  "name": "ofx4js",
  "version": "2.1.0",
  "description": "A javascript OFX library, ported from OFX4J",
  "license": "MIT",
  "author": "Andrew Olson <andrew@olsononline.org>",
  "repository": {
    "type": "git",
    "url": "https://github.com/arolson101/ofx4js.git"
  },
  "main": "src/index.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "npm run test && npm run build && npm run typedoc",
